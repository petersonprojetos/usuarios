		<main class="main-content">
			<div class="usuarios">
				<h1>Log</h1>
			</div>

			<section class="col-md-12" id="section2" style="background-color: #FFF;">

				<div id="informacoesGerais">
					<h3 style="color: #C63131">Informações cadastradas de usuários</h3>
				</div>

				<table id="tblDados" class="records">
					<thead>
						<tr>
							<th>ID</th>
							<th>Nome</th>
							<th>Login</th>
							<th>CPF</th>
							<th>Email</th>
							<th>Senha</th>
							<th>Bloqueado</th>
							<th>Perfis</th>
							<th>Criado</th>
							<th>Editado</th>
						</tr>
					</thead>

                    <tbody>
                        {{#each cadastros}}
                         <tr>
							<td> {{id}}  </td>
                            <td> {{nome}} </td>
                            <td> {{login}} </td>
                            <td> {{cpf}} </td>
                            <td> {{email}} </td>
                            <td> {{senha}} </td>
                            <td> {{bloqueado}} </td>
                            <td> {{perfis}}  </td>
                            <td> {{createdAt}} </td>
                            <td> {{updatedAt}} </td>
                         </tr>
                        {{/each}}
                    </tbody>
								
				</table>
				
			</section>	
		</main>

